{
    "name": "jsmind",
    "version": "2.0.0",
    "description": "Version 2 of jsMind, which is a TypeScript library for mindmap.",
    "type": "module",
    "main": "dist/jsmind.js",
    "types": "dist/jsmind.d.ts",
    "exports": {
        ".": {
            "import": "./dist/jsmind.js",
            "require": "./dist/jsmind.js",
            "types": "./dist/jsmind.d.ts"
        },
        "./jsmind.css": "./dist/jsmind.css"
    },
    "directories": {
        "doc": "docs",
        "example": "example"
    },
    "scripts": {
        "server": "http-server",
        "build": "rollup -c .config/rollup.config.js",
        "test": "node --test",
        "test-lite": "node --test --test-reporter=dot",
        "format": "eslint --fix --config .config/eslint.config.js",
        "format-check": "eslint --config .config/eslint.config.js",
        "type-check": "tsc --noEmit -p tsconfig.json",
        "compile": "tsc --help",
        "ci": "npm run format-check && npm run type-check && npm run test-lite && npm run build",
        "ci2": "npm run format-check && npm run type-check && npm run test-lite"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hizzgdev/jsmind.git"
    },
    "author": {
        "name": "hizzgdev@163.com"
    },
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/hizzgdev/jsmind/issues"
    },
    "homepage": "https://github.com/hizzgdev/jsmind#readme",
    "keywords": [
        "jsmind",
        "mindmap"
    ],
    "maintainers": [
        {
            "name": "hizzgdev",
            "email": "hizzgdev@163.com"
        }
    ],
    "devDependencies": {
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^9.39.2",
        "@eslint/json": "^0.14.0",
        "@rollup/plugin-typescript": "^12.3.0",
        "@stylistic/eslint-plugin": "^5.7.0",
        "@types/jsdom": "^27.0.0",
        "@types/node": "^25.0.5",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "eslint": "^9.39.2",
        "jsdom": "^27.4.0",
        "rollup": "^4.55.1",
        "rollup-plugin-dts": "^6.3.0",
        "tslib": "^2.8.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.52.0"
    }
}
