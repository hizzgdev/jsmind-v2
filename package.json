{
    "name": "jsmind",
    "version": "2.0.0",
    "description": "Version 2 of jsMind, which is a pure javascript library for mindmap.",
    "type": "module",
    "main": "dist/jsmind.js",
    "exports": {
        ".": "./dist/jsmind.js",
        "./jsmind.css": "./dist/jsmind.css"
    },
    "directories": {
        "doc": "docs",
        "example": "example"
    },
    "scripts": {
        "server": "http-server",
        "build": "rollup -c .config/rollup.config.js",
        "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
        "format": "npx eslint --fix",
        "format-check": "npx eslint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hizzgdev/jsmind.git"
    },
    "author": {
        "name": "hizzgdev@163.com"
    },
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/hizzgdev/jsmind/issues"
    },
    "homepage": "https://github.com/hizzgdev/jsmind#readme",
    "keywords": [
        "jsmind",
        "mindmap"
    ],
    "maintainers": [
        {
            "name": "hizzgdev",
            "email": "hizzgdev@163.com"
        }
    ],
    "devDependencies": {
        "eslint": "^9.21.0",
        "@eslint/js": "^9.21.0",
        "@eslint/json": "^0.7.0",
        "@stylistic/eslint-plugin": "^4.0.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0"
    }
